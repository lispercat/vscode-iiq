{
	"name": "vscode-iiq",
	"displayName": "vscode-iiq",
	"description": "Support for IdentityIQ development",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"languages": [
			{
				"id": "beanshell",
				"aliases": [
					"Beanshell",
					"beanshell"
				],
				"extensions": [
					".xml"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "beanshell",
				"scopeName": "text.xml.beanshell",
				"path": "./syntaxes/beanshell.tmLanguage.json",
				"injectTo": [
					"text.xml"
				],
				"embeddedLanguages": {
					"meta.embedded.block.beanshell": "java"
				}
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.helloWorld",
					"when": "resourceExtname == .xml",
					"group": "iiq"
				}
			],
			"editor/context": [
				{
					"command": "extension.helloWorld",
					"when": "resourceExtname == .xml",
					"group": "iiq"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.45.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}